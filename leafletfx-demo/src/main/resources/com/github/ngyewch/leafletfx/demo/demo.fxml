<?xml version="1.0" encoding="UTF-8"?>

<?import com.github.ngyewch.leafletfx.LeafletMapView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<VBox prefHeight="768.0" prefWidth="1024.0" spacing="16.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.ngyewch.leafletfx.demo.Controller">
    <children>
        <StackPane fx:id="spMapView" VBox.vgrow="ALWAYS">
            <AnchorPane>
                <LeafletMapView fx:id="leafletMapView" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                <HBox AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onStart" text="Start"/>
                        <Button mnemonicParsing="false" onAction="#onFinish" text="Finish"/>
                    </children>
                </HBox>
                <HBox AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0">
                    <children>
                        <Label fx:id="locationLabel"/>
                    </children>
                </HBox>
            </AnchorPane>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
</VBox>
